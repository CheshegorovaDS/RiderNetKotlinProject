<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.ridernetkotlinproject">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.example.ridernetkotlinproject.presentation.entry.EntryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.ridernetkotlinproject.presentation.registration.RegistrationActivity"/>
        <activity android:name="com.example.ridernetkotlinproject.presentation.login.activity.LogInActivity"/>
        <activity android:name=".presentation.menu.MenuActivity"/>
    </application>

</manifest>